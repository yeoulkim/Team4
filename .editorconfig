root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = space
indent_size = 4

[*.h]
indent_style = space
indent_size = 4

[*.cpp]
indent_style = space
indent_size = 4

[*.json]
indent_style = space
indent_size = 2

[*.ini]
indent_style = space
indent_size = 2
